.g-status-bar .wrap .box .item1.type3 .menu .item-index{ flex-grow: 1; flex-basis: 0; -webkit-flex-basis: 0; }
.g-status-bar .wrap .box .item2{ font-size: 32rpx; line-height: 44rpx; }
.g-status-bar.default .wrap .box .item1 .menu .item-back{ background: url("https://6661-fatlossclock-9gpwsn3r2ed98f31-1304792319.tcb.qcloud.la/static/img/2020052201.png") no-repeat center/36rpx auto; }
