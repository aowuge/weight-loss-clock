/* 透明度 */
.g-opacity0{ opacity: 0; filter: alpha(opacity=0); }
.g-opacity100{ opacity: 1; filter: alpha(opacity=100); }

/* 字体加粗 */
.g-font-500{ font-family: 'alibaba-medium'; font-weight: 500; }
