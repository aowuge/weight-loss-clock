/* 弹窗 */
.g-layer-shadow{ background: rgba(0,0,0,0.56); top: 0; left: 0; bottom: 0; right: 0; }
.g-layer-bottom{ bottom: 0; right: 0; left: 0; transform: translateY(120%); -webkit-transform: translateY(120%); opacity: 0; transition: all 0.4s; -webkit-transition: all 0.4s; }
.g-layer-bottom.active{ transform: translateY(0%); -webkit-transform: translateY(0%); opacity: 1; }
.g-layer-center{ top: 50%; left: 50%; transform: scale(0, 0) translate(-50%, -50%); transform-origin: 0 0 0; opacity: 0; transition: all 0.3s; -webkit-transition: all 0.3s; }
.g-layer-center.active{ transform: scale(1, 1) translate(-50%, -50%); opacity: 1; }
.g-layer-right{ top: 0; right: 0; bottom: 0; transform: translateX(100%); -webkit-transform: translateX(100%); transition: all 0.3s; -webkit-transition: all 0.3s; }
.g-layer-right.active{ transform: translateX(0); -webkit-transform: translateX(0); }


