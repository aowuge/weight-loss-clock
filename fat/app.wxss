/* 表单、按钮等 */
.g-btn{ color: #ffffff; background: linear-gradient(270deg, #6CE2D2 0%, #3BB19B 100%); box-shadow: 0 0 20rpx 2rpx rgba(93, 211, 193, 0.49); }

/* 色值 */
.g-c-aaa{ color: #AAAAAA; }
.g-c-666{ color: #666666; }
.g-c-777{ color: #777777; }
.g-c-999{ color: #999999; }
.g-c-white{ color: #ffffff; }
