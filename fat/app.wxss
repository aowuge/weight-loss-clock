page { width: 100%; height: 100%; font-family: 'alibaba'; color: #333; }

/* 默认 */
page button{ min-height: initial; padding: 0; line-height: initial; font-weight: initial; font-size: initial; border-radius: initial; background: initial; text-align: initial; }
page button::after{ border: none; }
page .button-hover{ width: 1rpx; height: 1rpx; background: transparent; }
page input,page textarea{ color: #333; height: initial; min-height: initial; width: initial; }
page .g-placeholder{ color: #CCCCCC; font-size: 28rpx; }
::-webkit-scrollbar {  width: 0;  height: 0; color: transparent;}

/* 基础 */
.tc { text-align: center; }
.tr { text-align: right; }
.tl { text-align: left; }
.pf { position: fixed; }
.pr { position: relative; }
.po { position: absolute; }
.g-pointer { cursor: pointer; -webkit-tap-highlight-color: rgba(255, 255, 255, 0); }
.g-of-ellipsis{ display:block; text-overflow:ellipsis; -o-text-overflow:ellipsis; -webkit-text-overflow:ellipsis; -moz-text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}
.g-of-ellipsis2{ text-overflow: -o-ellipsis-lastline; overflow: hidden;text-overflow: ellipsis; display: -webkit-box;-webkit-line-clamp: 2;line-clamp: 2;-webkit-box-orient: vertical;}
.g-of-hide{ overflow: hidden; }

/* flex */
.g-flex { display: -webkit-box; display: flex; display: -ms-flex; display: -webkit-flex; }
.g-flex-grow1 { flex-grow: 1; flex-basis: 0; -webkit-flex-basis: 0; }
.g-flex-grow2 { flex-grow: 2; flex-basis: 0; -webkit-flex-basis: 0; }
.g-flex-d-row{ flex-direction: row; } /* 默认值。 flex项目水平显示为一行 */
.g-flex-d-row-reverse{ flex-direction: row-reverse; } /* 与行相同，但顺序相反 */
.g-flex-d-column{ flex-direction: column; } /* 灵活flex项目作为列垂直显示 */
.g-flex-d-column-reverse{ flex-direction: column; } /* 与列相同，但顺序相反 */
.g-flex-d-column-initial{ flex-direction: initial; } /* 将此属性设置为其默认值 */
.g-flex-d-column-inherit{ flex-direction: column; } /* 从其父元素继承此属性 */
.g-flex-i-center{ align-items: center; }
.g-flex-i-start{ align-items: flex-start; }
.g-flex-i-end{ align-items: flex-end; }
.g-flex-c-around{ justify-content: space-around; }
.g-flex-c-between{ justify-content: space-between; }
.g-flex-c-center{ justify-content: center; }
.g-flex-c-end{ justify-content: flex-end; }
.g-flex-wrap{ flex-wrap: wrap; }
.g-flex-nowrap{ flex-wrap: nowrap; }

/* 宽高 */
.g-w-full{ width: 100%; }
.g-h-full{ height: 100%; }
.g-wh-full{ width: 100%; height: 100%; }

/* 层级 */
.z-index1{ z-index: 1; }
.z-index9{ z-index: 9; }
.z-index99{ z-index: 99; }
.z-index999{ z-index: 999; }
.z-index10000{ z-index: 10000; }
.z-index10001{ z-index: 10001; }

